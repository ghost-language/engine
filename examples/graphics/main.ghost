function load() {
    Window.title("Graphics Demo")
}

function draw() {
    Graphics.draw('./examples/graphics/player.png', 100, 100)

    Graphics.setColor(255, 0, 0)
    Graphics.pixel(400, 300)
    Graphics.rectangle(300, 100, 100, 100)

    Graphics.setColor(0, 0, 255)
    Graphics.filledRectangle(500, 100, 100, 100)

    Graphics.setColor(255, 255, 255)
    Graphics.line(500, 500, 300, 400)
}